[core]
  editor = vim
  autocrlf = input
  quotepath = false
	excludesfile = /Users/wate/.gitignore
  attributesfile = ~/.gitattributes
[fetch]
  prune = True
[pull]
  rebase = True
[push]
  default = simple
[filter "lfs"]
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
  clean = git-lfs clean -- %f
[alias]
	au = add --update
	aa = add --all
	ap = add --patch
	ai = add --interactive
	br = branch
	co = checkout
	ci = commit
	ca = commit --amend
	st = status -s
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %Cblue<%an>%Creset' --abbrev-commit --date=relative --all --decorate
	cp = cherry-pick
	dc = diff --cached
	dw = diff --word-diff
	last = log -1 --stat
	unstage = reset HEAD --
	ignore = "!gi() { curl -L -s https://www.gitignore.io/api/$@ ;}; gi"
	assume = update-index --assume-unchanged
	unassume = update-index --no-assume-unchanged
	assumed = !git ls-files -v | grep ^h | cut -c 3-
	delete-merged-remote-branches = !git branch -a --merged | grep -vE \"master|develop\" | sed -e \"s% *remotes/origin/%%\" | xargs -I% git push origin :%
	delete-merged-branches = !git branch --merged | grep -vE \"master|develop\"  | xargs -I % git branch -d %
[difftool "sourcetree"]
	cmd = /Applications/p4merge.app/Contents/MacOS/p4merge \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/p4merge.app/Contents/MacOS/p4merge \"$BASE\" \"$LOCAL\" \"$REMOTE\" \"$MERGED\"
	trustExitCode = true
